{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true,
		"jest": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"sonarjs/no-identical-expressions": "off",
		"linebreak-style": "off",
		"no-console": "off",
		"max-len": 0,
		"no-underscore-dangle": "off"
	}
}
